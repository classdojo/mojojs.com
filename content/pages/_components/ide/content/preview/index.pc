<div class="ide-preview col-sm-4">
  <div class="row-fluid">
    <div class="col-sm-12">
      {{#if:loading}}
        loading preview...
      {{/elseif:error}}
        <div class="alert alert-danger">
          {{error.message}}
        </div>
      {{/}}
      {{#if:script}}
        {{ html: content }}
      {{/}}
    </div>
  </div>
</div>